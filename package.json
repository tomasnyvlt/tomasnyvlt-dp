{
  "name": "@tomasnyvlt/tomasnyvlt-dp",
  "version": "0.0.12",
  "type": "module",
  "license": "MIT",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "typescript",
    "ui",
    "components",
    "chromatic",
    "storybook",
    "vite",
    "vitest",
    "eslint",
    "prettier"
  ],
  "scripts": {
    "dev": "vite --force",
    "build": "vite build && pnpm build:types",
    "build:types": "tsc && tsc-alias -p tsconfig.json",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest --run",
    "storybook": "storybook dev -p 6006",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "build-storybook": "storybook build -o storybook-static"
  },
  "dependencies": {
    "@data-driven-forms/common": ">=3",
    "@data-driven-forms/react-form-renderer": ">=3",
    "@elgorditosalsero/react-gtm-hook": "^2.7.2",
    "@tippyjs/react": "^4.2.6",
    "anolis-ui": "^0.3.4",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "deep-object-diff": "^1.1.9",
    "draft-js": "^0.11.7",
    "draft-js-export-html": "^1.4.1",
    "framer-motion": "^10.18.0",
    "html-react-parser": "^2.0.0",
    "react-datepicker": "^4.25.0",
    "react-final-form-listeners": "^1.0.3",
    "react-icons": "^4.12.0",
    "react-loading-skeleton": "^3.4.0",
    "react-number-format": "^5.3.4",
    "react-router-dom": "^6.23.1",
    "react-select": "^5.8.0",
    "tsc-alias": "^1.8.10",
    "zustand": "^4.5.2"
  },
  "peerDependencies": {
    "@xstyled/emotion": "3.6.0",
    "@xstyled/system": "3.6.0",
    "next": ">=14",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@storybook/addon-essentials": "^7.6.19",
    "@storybook/addon-interactions": "^7.6.19",
    "@storybook/addon-links": "^7.6.19",
    "@storybook/blocks": "^7.6.19",
    "@storybook/builder-vite": "^7.6.19",
    "@storybook/react": "^7.6.19",
    "@storybook/react-vite": "^7.6.19",
    "@storybook/testing-library": "0.0.14-next.2",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.3.1",
    "@types/draft-js": "^0.11.18",
    "@types/react": "^18.3.3",
    "@types/react-datepicker": "^4.19.6",
    "@types/react-dom": "^18.3.0",
    "@types/react-final-form-listeners": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "chromatic": "^6.24.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.6.15",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^7.6.19",
    "typescript": "^5.4.5",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
